#include<stdio.h>
int main()
{
    int n,i,sumfac=0,f=1,a[50];
    scanf("%d",&n);
    printf("Enter elements\n");
    for(i=0;i<n;i++)
    {
    	f=1;
    	scanf("%d",&a[i]);
    	while(a[i]!=0)
		{
			f=f*a[i];
			a[i]--;
		}
		sumfac+=f;
	}
		printf("Sum of the factors of elements is %d\n",sumfac);
    return 0;
}
